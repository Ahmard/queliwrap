{
    "name": "ahmard/queliwrap",
    "description": "QueryList PHP web scrapper wrapper.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,

    "require": {
        "php": "^7.4 || ^8.0",
        "jaeger/querylist": "^4.2",
        "ahmard/guzwrap": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Queliwrap\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.64",
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Queliwrap\\Tests\\": "tests/"
        }
    }
}
